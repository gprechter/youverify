privateData1, privateData2: ARRAY{INT}
privateData1_length, privateData2_length: INT
buf1, buf2: ARRAY{INT}
buf1_length, buf2_length: INT

i: INT
k: INT
t: INT

privateData1 = ARRAY[8]{$sym{INT}}
privateData1_length = 0
privateData2 = ARRAY[8]{$sym{INT}}
privateData2_length = 0
k = 8
i = 0
LABEL LOOP1: if i >= k goto END1
begin_merge
privateData1[i] = $sym{INT}
privateData1_length = privateData1_length + 1
privateData2[i] = $sym{INT}
privateData2_length = privateData2_length + 1
end_merge
i = i + 1
goto LOOP1
LABEL END1: i = 0

buf1 = ARRAY[]{0}
buf1_length = 0
LABEL LOOP2: if i >= k goto END2
begin_merge
t = privateData1[i]
if t < 0 goto ELSE
buf1[buf1_length] = 1
buf1_length = buf1_length + 1
goto ENDCOND
LABEL ELSE: buf1[buf1_length] = 0
buf1_length = buf1_length + 1
end_merge
LABEL ENDCOND: i = i + 1
goto LOOP2
LABEL END2: i = 0

buf2 = ARRAY[]{0}
buf2_length = 0
LABEL LOOP3: if i >= k goto END3
begin_merge
t = privateData2[i]
if t < 0 goto ELSE2
buf2[buf2_length] = 1
buf2_length = buf2_length + 1
goto ENDCOND2
LABEL ELSE2: buf2[buf2_length] = 0
buf2_length = buf2_length + 1
end_merge
LABEL ENDCOND2: i = i + 1
goto LOOP3
LABEL END3: t = 0

verify buf1_length == buf2_length